<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.exp.homedelivery">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".Activities.Admin.AdminOrderDetails" />
        <activity android:name=".Activities.Admin.AdminReceivedOrders" />
        <activity android:name=".Activities.Admin.AdminLogin" />
        <activity android:name=".Activities.User.OrderDetails" />
        <activity android:name=".Activities.User.MyProfile" />
        <activity android:name=".Activities.User.MyOrder" />
        <activity android:name=".Activities.User.Payment" />
        <activity android:name=".Activities.User.FinalCheckout" />
        <activity android:name=".Activities.User.ShoppingCart" />
        <activity android:name=".Activities.User.HomeDeliveryMenu" />
        <activity android:name=".Activities.User.HomeDeliveryTeamSearch" />
        <activity android:name=".Activities.User.Login" />
        <activity android:name=".Activities.User.Registration" />
    </application>

</manifest>